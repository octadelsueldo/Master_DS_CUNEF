fyb <- function(x) fy(x[1],x[2])
sumssq <- function(x) fxb(x)**2+fyb(x)**2
optim(c(-2,2),sumssq)$par ##(this is almost 0,0)
optim(c(-1,1),sumssq)$par
optim(c(-0.5,0.5),sumssq)$par
